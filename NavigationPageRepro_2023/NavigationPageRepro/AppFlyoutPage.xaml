<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                  xmlns:local="clr-namespace:NavigationPageRepro"
                  x:Class="NavigationPageRepro.AppFlyoutPage"
                  x:DataType="local:AppFlyoutPage">
    <FlyoutPage.Flyout>
        <local:AppFlyoutPageMenu x:Name="appFlyoutPageMenu" />
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <local:MainPage1 />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>
